odoo.define('@knowledge/../tests/tours/knowledge_readonly_favorite_tour', ['@knowledge/../tests/tours/knowledge_main_flow_tour', '@knowledge/../tests/tours/knowledge_tour_utils', 'web_tour.tour'], async function (require) {
